{
    "builds": [
      {
        "src": "pdf_chat_app/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)", 
        "dest": "/pdf_chat_app/static/$1" 
      },
      {
        "src": "/(.*)",
        "dest": "pdf_chat_app/wsgi.py"
      }
    ]
  }
  